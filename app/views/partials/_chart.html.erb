<style>

.bar {
  fill: steelblue;
}

.bar:hover {
  fill: brown;
}

.axis {
  font: 10px sans-serif;
}

.axis path,
.axis line {
  fill: none;
  stroke: #000;
  shape-rendering: crispEdges;
}

.x.axis path {
  display: none;
}

</style>
<body>
<script src="http://d3js.org/d3.v3.min.js"></script>
<script>

var margin = {top: 20, right: 20, bottom: 30, left: 40},
    width = 960 - margin.left - margin.right,
    height = 500 - margin.top - margin.bottom;

var x = d3.scale.ordinal()
    .rangeRoundBands([0, width], .1);

var y = d3.scale.linear()
    .range([height, 0]);

var xAxis = d3.svg.axis()
    .scale(x)
    .orient("bottom");

var yAxis = d3.svg.axis()
    .scale(y)
    .orient("left")
    .ticks(10, "%");

var svg = d3.select("body").append("svg")
    .attr("width", width + margin.left + margin.right)
    .attr("height", height + margin.top + margin.bottom)
  .append("g")
    .attr("transform", "translate(" + margin.left + "," + margin.top + ")");

var data = [{timeOfTweet: 9, frequency: 0}, 
            {timeOfTweet: 10, frequency: 0}, 
            {timeOfTweet: 11, frequency: 0}, 
            {timeOfTweet: 12, frequency: 0}, 
            {timeOfTweet: 1, frequency: 0}, 
            {timeOfTweet: 2, frequency: 0}, 
            {timeOfTweet: 3, frequency: 0}, 
            {timeOfTweet: 4, frequency: 0}, 
            {timeOfTweet: 5, frequency: 0}, 
            {timeOfTweet: 6, frequency: 0}, 
            {timeOfTweet: 7, frequency: 0}, 
            {timeOfTweet: 8, frequency: 0}, 
            {timeOfTweet: 9, frequency: 0}, 
            {timeOfTweet: 10, frequency: 0}];
window.searchArray.forEach(function(tweet){
  if (tweet.chart > "02:00AM") && (tweet.chart < "03:00AM") {
    data[0]["frequency"] ++;
  } else if {
    tweet.chart > "03:00AM") && (tweet.chart < "04:00AM") {
    data[1]["frequency"] ++;
  }else if {
    tweet.chart > "04:00AM") && (tweet.chart < "05:00AM") {
    data[2]["frequency"] ++;
  }else if {
    tweet.chart > "05:00AM") && (tweet.chart < "06:00AM") {
    data[3]["frequency"] ++;
  }else if {
    tweet.chart > "06:00AM") && (tweet.chart < "07:00AM") {
    data[4]["frequency"] ++;
  }else if {
    tweet.chart > "07:00AM") && (tweet.chart < "08:00AM") {
    data[5]["frequency"] ++;
  }else if {
    tweet.chart > "08:00AM") && (tweet.chart < "09:00AM") {
    data[6]["frequency"] ++;
  }else if {
    tweet.chart > "09:00AM") && (tweet.chart < "10:00AM") {
    data[7]["frequency"] ++;
  }else if {
    tweet.chart > "10:00AM") && (tweet.chart < "11:00AM") {
    data[8]["frequency"] ++;
  }else if {
    tweet.chart > "11:00AM") && (tweet.chart < "12:00PM") {
    data[9]["frequency"] ++;
  }else if {
    tweet.chart > "12:00PM") && (tweet.chart < "01:00PM") {
    data[10]["frequency"] ++;
  }else if {
    tweet.chart > "01:00PM") && (tweet.chart < "02:00PM") {
    data[10]["frequency"] ++;
  }else if {
    tweet.chart > "02:00PM") && (tweet.chart < "03:00PM") {
    data[12]["frequency"] ++;
  }else if {
    tweet.chart > "03:00PM") && (tweet.chart < "04:00PM") {
    data[13]["frequency"] ++;
  }
  console.log(data)
})

x.domain(data.map(function(d) { return d.timeOfTweet; }));
y.domain([0, d3.max(data, function(d) { return d.frequency; })]);

svg.append("g")
    .attr("class", "x axis")
    .attr("transform", "translate(0," + height + ")")
    .call(xAxis);

svg.append("g")
    .attr("class", "y axis")
    .call(yAxis)
  .append("text")
    .attr("transform", "rotate(-90)")
    .attr("y", 6)
    .attr("dy", ".71em")
    .style("text-anchor", "end")
    .text("Frequency");

svg.selectAll(".bar")
    .data(data)
  .enter().append("rect")
    .attr("class", "bar")
    .attr("x", function(d) { return x(d.timeofTweet); })
    .attr("width", x.rangeBand())
    .attr("y", function(d) { return y(d.frequency); })
    .attr("height", function(d) { return height - y(d.frequency); });

function type(d) {
d.frequency = +d.frequency;
return d;
}

</script>